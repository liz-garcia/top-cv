    "@eslint/js": "^9.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.6.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "prettier-eslint": "^16.3.0"
